[user]
	email = nathan@turbot.com
	name = Nathan Wallace
[push]
	default = simple
[core]
	editor = /usr/bin/vim
[credential]
	helper = osxkeychain
[url "git@github.com:"]
	insteadOf = https://github.com/
[init]
	defaultBranch = main
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
